{
  "name": "mutelight",
  "version": "1.0.0",
  "description": "Discord mute status LED controller for WLED devices",
  "main": "dist-main/index.js",
  "author": "MuteLight",
  "license": "MIT",
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "vite build --config vite.config.main.ts",
    "build:preload": "vite build --config vite.config.preload.ts",
    "build:renderer": "vite build --config vite.config.renderer.ts",
    "package": "npm run build && electron-builder",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@xhayper/discord-rpc": "^1.1.0",
    "bonjour-service": "^1.3.0",
    "electron-store": "^8.1.0",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "winston": "^3.11.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/bonjour": "^3.5.13",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}
